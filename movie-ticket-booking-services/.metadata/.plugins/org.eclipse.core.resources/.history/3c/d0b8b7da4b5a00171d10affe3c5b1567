package com.it14031380.dsAssignment2.restaurant.models;

public class FoodItem {
	private String name;
	private float amount;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public float getAmount() {
		return amount;
	}
	public void setAmount(double d) {
		this.amount = d;
	}
}
